    marks=marks/10;
    
        switch(marks)
        {
         case 10:
         printf("A+ grade");
         break;
         
         case 9:
         printf("A grade");
         break;
         
         case 8:
         printf("B grade");
         break;
         
         case 7:
         printf("B grade");
         break;
         
        case 6:
         printf("C grade");
         break;
         
         case 5:
         printf("C grade");
         break;
         
         case 4:
         printf("D grade");
         break;
         
        case 3:
         printf("fail");
         break;
         
         default:
         printf("that's why you are fail");
         
        }
    
    
    
    return 0;
}
