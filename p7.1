#include <stdio.h>

void main()  
{
    int week;
    printf("enter number of week beween (1to7)\n");
    scanf("%d",&week);
    switch(week)
    {
        case 1:
        printf("Monday");
        break;
        
        case 2:
        printf("teusday");
        break;
        
        case 3:
        printf("wednesday");
        break;
        
        case 4:
        printf("thursday");
        break;
        
        case 5:
        printf("friday");
        break;
        
        case 6:
        printf("saturday");
        break;
        
        case 7:
        printf("sunday");
        break;
        
        default:
        printf("error\n please enter number (1 to 7)");
        
        
    }
    
    
    return 0;
}
